#include <stdio.h>
int main()
{
int age;
float h,w;
printf("enter your age:-");
scanf("%d",&age);
if(age>=18&&age<=35)
{printf("enter your hieght:-");
scanf("%f",&h);
printf("enter your wieght:-");
scanf("%f",&w);
if (h>=5.5,w<55)
{printf("you are eligible for ncc");
}
else
{printf("not eligible due to wieght and hieght ");
}
}
else if(age>35 && age<=45)
{
 printf("you are eligible for ncc office");
}
