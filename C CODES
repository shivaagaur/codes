program 3
int main()
{
int age;
float h,w;
printf("enter your age:-");
scanf("%d",&age);
if(age>=18&&age<=35)
{
printf("you are eligible for ncc");
}
else if(age>35 && age<=45)
{
 printf("you are eligible for ncc office");
}
else 
{
printf("you are not eligible");
}
 return 0;
}
