#include <stdio.h>
int compare(char a[],char b[]);
int main()
{char str1[10]="firozabad";
char str2[10]="firozabad";
int z=compare(str1,str2);
if(z==0)
{
printf("two string are equal");
}
else
{
printf("two strings are not equal");
}
}
int compare(char a[],char b[])
{ int i=0,x;
while(a[i]!='\0'&&b[i]!='\0')
{if(a[i]==b[i])
{x=0;
i++;
}
else
{x=1;
}
return x;
}
}
