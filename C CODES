#include <stdio.h>
int main()
{int n,temp,sum=0,rem;
printf("input the no");
scanf("%d",&n);
temp=n;
while(n>0)
{rem=n%10;
sum=(sum*10)+rem;
n=n/10;
}
if (temp==sum)
{printf("the give no is palindrome number");
}
else
{printf("the given no is not a palindrome number");
}
 return 0;
}
